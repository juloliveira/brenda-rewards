@section scripts {

    <script src="~/assets/plugins/custom/fullcalendar/fullcalendar.bundle.js?v=7.0.6"></script>

    <script src="~/assets/js/highcharts/highcharts.js"></script>
    <script src="~/assets/js/highcharts/modules/drilldown.js"></script>

    <script src="~/assets/js/pages/dashboard.js"></script>

    <script>
        var sex = @Html.Raw(ViewBag.Sexualities)
        var incomes = @Html.Raw(ViewBag.Incomes)
        var schooling = @Html.Raw(ViewBag.Schooling)
        var result = [];
        var result2 = [];
        var d = sex.reduce(function (res, value) {
            if (!res[value.Code]) {
                res[value.Code] = { code: value.Code, description: value.Description, amount: 0, drilldown: [] };
                result.push(res[value.Code]);
            }

            if (!res[value.Date]) {
                res[value.Date] = { date: value.Date, men: 0, women: 0 };
                result2.push(res[value.Date]);
            }

            if (value.Code == 1) res[value.Date].men += value.Amount;
            if (value.Code == 2) res[value.Date].women += value.Amount;

            res[value.Code].amount += value.Amount;
            res[value.Code].drilldown.push([value.Date, value.Amount]);
            return res;
        }, {});

        var incomeDates = []
        var ddd = incomes.reduce(function (res, value) {
            if (!res[value.Date]) {
                res[value.Date] = { date: value.Date };
                incomeDates.push(value.Date);
            }
            return res;
        });

        var incomeSeries = [];
        incomes.map(function (item) {
            if (!incomeSeries.some(x => x.id == item.IncomeId)) {
                incomeSeries.push({ id: item.IncomeId, name: item.Description, data: [0, 0, 0, 0, 0, 0, 0, 0, 0] });
            } 


            var xIndex = incomeDates.indexOf(item.Date);
            incomeSeries.find(x => x.id === item.IncomeId).data[xIndex] = item.Amount;
        });

        
        //console.log(incomes);

        Highcharts.chart('incomes', {
            chart: { type: 'column' },
            title: { text: 'Requisições por Renda' },
            xAxis: {
                categories: incomeDates
            },
            series: incomeSeries
        });

        Highcharts.chart('menwoman-bars', {
            chart: {
                type: 'column'
            },
            title: {
                text: 'Requisições entre Homens x Mulheres'
            },
            xAxis: {
                categories: result2.map(function (item) { return item.date }),
                crosshair: true
            },
            plotOptions: {
                column: {
                    pointPadding: 0.2,
                    borderWidth: 0
                }
            },
            series: [{
                name: 'Homens',
                data: result2.map(function (item) { return item.men; })

            }, {
                name: 'Mulheres',
                data: result2.map(function (item) { return item.women })

            }]
        });

        Highcharts.chart('menwoman-drill', {
            chart: { type: 'column' },
